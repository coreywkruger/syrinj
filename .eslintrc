{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "es6": true,
    "modules": true,
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "$": true,
    "inject": true
  },
  "extends": [
    "gobold",
    "gobold/es6",
  ],
  "rules": {
    "camelcase": 0,
    "consistent-this": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "no-multi-spaces": 1,
    "no-redeclare": 1,
    "no-shadow": 1,
    "no-undef": 1,
    "no-use-before-define": 1,
    "quotes": 1,
    "semi": 1,
  }
}
